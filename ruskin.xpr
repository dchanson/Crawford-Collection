<?xml version="1.0" encoding="UTF-8"?>
<project>
    <meta>
        <!-- Filters now point to crawford.xpr -->
        <filters directoryPatterns="" filePatterns="crawford.xpr" positiveFilePatterns="" showHiddenFiles="false"/>
        <options>
            <serialized xml:space="preserve">
                <serializableOrderedMap>
                    <entry>
                        <String>enable.project.master.files.support</String>
                        <Boolean>true</Boolean>
                    </entry>
                    <entry>
                        <String>scenarios</String>
                        <scenario-array>

                            <!-- Scenario 1: Global Generate -->
                            <scenario>
                                <field name="advancedOptionsMap"><null/></field>
                                <field name="name"><String>Generate (global)</String></field>
                                <field name="baseURL"><String></String></field>
                                <field name="footerURL"><String></String></field>
                                <field name="fOPMethod"><String>pdf</String></field>
                                <field name="fOProcessorName"><String>Apache FOP</String></field>
                                <field name="headerURL"><String></String></field>
                                <field name="inputXSLURL"><String>${pdu}/common/crawford.xsl</String></field>
                                <field name="inputXMLURL"><String>${currentFileURL}</String></field>
                                <field name="defaultScenario"><Boolean>false</Boolean></field>
                                <field name="isFOPPerforming"><Boolean>false</Boolean></field>
                                <field name="type"><String>XSL</String></field>
                                <field name="saveAs"><Boolean>true</Boolean></field>
                                <field name="openInBrowser"><Boolean>false</Boolean></field>
                                <field name="outputResource"><String>${pd}/gen/${makeRelative(${pd}, ${cfd})}/${cfn}.php</String></field>
                                <field name="openOtherLocationInBrowser"><Boolean>true</Boolean></field>
                                <field name="locationToOpenInBrowserURL"><String>http://crawford.local:8080/src/crawford_redir_file.php?open=${cf}</String></field>
                                <field name="openInEditor"><Boolean>false</Boolean></field>
                                <field name="showInHTMLPane"><Boolean>false</Boolean></field>
                                <field name="showInXMLPane"><Boolean>false</Boolean></field>
                                <field name="showInSVGPane"><Boolean>false</Boolean></field>
                                <field name="showInResultSetPane"><Boolean>false</Boolean></field>
                                <field name="useXSLTInput"><Boolean>true</Boolean></field>
                                <field name="xsltParams">
                                    <list>
                                        <transformationParameter>
                                            <field name="paramDescription">
                                                <paramDescriptor>
                                                    <field name="localName"><String>outputFullWitnessPage</String></field>
                                                    <field name="prefix"><null/></field>
                                                    <field name="namespace"><null/></field>
                                                </paramDescriptor>
                                            </field>
                                            <field name="value"><String>1</String></field>
                                            <field name="hasXPathValue"><Boolean>false</Boolean></field>
                                            <field name="isStatic"><Boolean>false</Boolean></field>
                                        </transformationParameter>
                                    </list>
                                </field>
                                <field name="cascadingStylesheets"><String-array/></field>
                                <field name="xslTransformer"><String>Saxon-EE</String></field>
                                <field name="extensionURLs"><String-array/></field>
                            </scenario>

                            <!-- Scenario 2: Generate and open HTML -->
                            <scenario>
                                <field name="advancedOptionsMap"><null/></field>
                                <field name="name"><String>Generate and open html</String></field>
                                <field name="inputXSLURL"><String>${pdu}/common/crawford_html.xsl</String></field>
                                <field name="inputXMLURL"><String>${currentFileURL}</String></field>
                                <field name="saveAs"><Boolean>true</Boolean></field>
                                <field name="openInBrowser"><Boolean>true</Boolean></field>
                                <field name="outputResource"><String>${pd}/gen/${makeRelative(${pd}, ${cfd})}/${cfn}.php</String></field>
                                <field name="openOtherLocationInBrowser"><Boolean>true</Boolean></field>
                                <field name="locationToOpenInBrowserURL"><String>http://crawford.local:8080/src/crawford_redir_file.php?open=${cf}&amp;new=1</String></field>
                                <field name="useXSLTInput"><Boolean>true</Boolean></field>
                                <field name="xsltParams">
                                    <list>
                                        <transformationParameter>
                                            <field name="paramDescription">
                                                <paramDescriptor>
                                                    <field name="localName"><String>contentOnly</String></field>
                                                    <field name="prefix"><null/></field>
                                                    <field name="namespace"><null/></field>
                                                </paramDescriptor>
                                            </field>
                                            <field name="value"><String>1</String></field>
                                            <field name="hasXPathValue"><Boolean>false</Boolean></field>
                                            <field name="isStatic"><Boolean>false</Boolean></field>
                                        </transformationParameter>
                                    </list>
                                </field>
                                <field name="xslTransformer"><String>Saxon-EE</String></field>
                            </scenario>

                            <!-- Scenario 3: Generate HTML only -->
                            <scenario>
                                <field name="advancedOptionsMap"><null/></field>
                                <field name="name"><String>Generate html</String></field>
                                <field name="inputXSLURL"><String>${pdu}/common/crawford_html.xsl</String></field>
                                <field name="inputXMLURL"><String>${currentFileURL}</String></field>
                                <field name="saveAs"><Boolean>true</Boolean></field>
                                <field name="openInBrowser"><Boolean>false</Boolean></field>
                                <field name="outputResource"><String>${pd}/gen/${makeRelative(${pd}, ${cfd})}/${cfn}.html</String></field>
                                <field name="openOtherLocationInBrowser"><Boolean>true</Boolean></field>
                                <field name="locationToOpenInBrowserURL"><String>http://crawford.local:8080/src/crawford_redir_file.php?open=${cf}&amp;new=1</String></field>
                                <field name="useXSLTInput"><Boolean>true</Boolean></field>
                                <field name="xsltParams">
                                    <list>
                                        <transformationParameter>
                                            <field name="paramDescription">
                                                <paramDescriptor>
                                                    <field name="localName"><String>contentOnly</String></field>
                                                    <field name="prefix"><null/></field>
                                                    <field name="namespace"><null/></field>
                                                </paramDescriptor>
                                            </field>
                                            <field name="value"><String>1</String></field>
                                            <field name="hasXPathValue"><Boolean>false</Boolean></field>
                                            <field name="isStatic"><Boolean>false</Boolean></field>
                                        </transformationParameter>
                                    </list>
                                </field>
                                <field name="xslTransformer"><String>Saxon-EE</String></field>
                            </scenario>

                        </scenario-array>
                    </entry>
                    <entry>
                        <String>storage.level.scenario.associations</String>
                        <Boolean>false</Boolean>
                    </entry>
                </serializableOrderedMap>
            </serialized>
        </options>
    </meta>

    <!-- Project tree updated for Crawford -->
    <projectTree name="crawford.xpr">
        <folder masterFiles="true" name="Master Files">
            <file name="common/crawford.rnc"/>
            <file name="../Documents/crawford.ste"/>
            <file name="common/crawford.xsl"/>
        </folder>
        <folder path="."/>
        <file name="_xml/_Completed/witnesses/example1.xml"/>
        <file name="_xml/_Completed/witnesses/example2.xml"/>
    </projectTree>
</project>
